{
  "name": "@vibex/core",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external @vibex/data --external @vibex/tools",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "test": "vitest"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.45",
    "@ai-sdk/deepseek": "^1.0.29",
    "@ai-sdk/openai": "^2.0.71",
    "@openrouter/ai-sdk-provider": "^1.2.5",
    "@vibex/data": "workspace:^",
    "@vibex/tools": "workspace:^",
    "ai": "^5.0.52",
    "nanoid": "^5.0.0",
    "playwright": "^1.56.1",
    "uuid": "^13.0.0",
    "yaml": "^2.8.1",
    "zod": "^3.22.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "^11.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
