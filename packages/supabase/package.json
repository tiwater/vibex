  "dependencies": {
    "@supabase/supabase-js": "^2.47.2",
    "@vibex/data": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
