{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "pnpm install --frozen-lockfile && pnpm turbo run build --filter=@vibex/docs"
  },
  "deploy": {
    "startCommand": "cd docs && pnpm start",
    "healthcheckPath": "/",
    "healthcheckTimeout": 300
  }
}
