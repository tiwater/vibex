##############################################

# Filled when you run `pnpm basic`, but we provide sane defaults so you can see the format
SPACE_ID=proj_demo_space

# Thesis Writer resume support
THESIS_SPACE_ID=proj_demo_thesis

# Research Assistant resume support
RESEARCH_SPACE_ID=proj_demo_research

# Code Review resume support
REVIEW_SPACE_ID=proj_demo_review

