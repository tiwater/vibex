# Thesis Writer Environment Template
#
# Copy to `.env` and set the values before running:
#
#   cp env.example .env
#   pnpm start

# LLM Provider (OpenRouter is the default in VibeX)
OPENROUTER_API_KEY=sk-or-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# Optional observability
HELICONE_API_KEY=

# Set this after running once so you can resume sessions
THESIS_SPACE_ID=

